install -dm755 "$PKGDIR"/etc/autofs/auto.master.d
install -dm755 "$PKGDIR"/usr/lib/systemd/system
cp -v "$SRCDIR"/samples/auto.* "$PKGDIR"/etc/autofs
cp -v "$SRCDIR"/samples/*.conf "$PKGDIR"/etc/autofs
cp -v "$SRCDIR"/samples/*.service "$PKGDIR"/usr/lib/systemd/system
cp -v "$SRCDIR"/samples/autofs.conf.default "$PKGDIR"/etc/autofs/autofs.conf
