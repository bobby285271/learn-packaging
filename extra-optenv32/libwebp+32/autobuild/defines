PKGNAME=libwebp+32
PKGDES="Libraries and support programs to encode and decode images in WebP format (optenv32)"
PKGDEP="libjpeg-turbo+32 libpng+32 libtiff+32 giflib+32 freeglut+32"
PKGSEC=libs

AUTOTOOLS_AFTER="--enable-experimental --enable-libwebpmux \
                 --enable-libwebpdemux --enable-libwebpdecoder"
NOLTO=yes
ABHOST=noarch
