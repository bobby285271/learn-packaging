PKGNAME=imlib2+32
PKGDES="Image loading, saving, rendering, and manipulation library (optenv32)"
PKGDEP="libtiff+32 giflib+32 bzip2+32 freetype+32 x11-lib+32 libpng+32 libid3tag+32 libjpeg-turbo+32"
PKGSEC=libs

AUTOTOOLS_AFTER="--sysconfdir=/etc/imlib2 --x-libraries=/usr/lib"
NOLTO=yes
ABHOST=noarch
