PKGNAME=taglib+32
PKGDES="Library for reading and editing the meta-data of several popular audio formats (optenv32)"
PKGSEC=libs
PKGDEP="zlib+32"
BUILDDEP="32subsystem"

CMAKE_AFTER="-DWITH_MP4=ON -DWITH_ASF=ON \
             -DBUILD_SHARED_LIBS=ON \
             -DHAVE_BOOST_BYTESWAP=NO -DHAVE_BOOST_ATOMIC=NO -DHAVE_BOOST_ZLIB=NO"
NOLTO=yes
ABHOST=noarch
