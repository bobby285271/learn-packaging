PKGNAME=cairo+32
PKGDEP="x11-lib+32 libdrm+32 libpng-1.6+32 fontconfig+32 glib+32 pixman+32 libspectre+32 poppler+32 librsvg+32"
BUILDDEP="32subsystem"
PKGSEC=libs
PKGDES="A 2D graphics library with support for multiple output devices."

AUTOTOOLS_AFTER="--enable-gl --enable-tee --disable-static --enable-egl \
                 --enable-svg --enable-ps --enable-pdf --enable-gobject --disable-lto \
                 --disable-test-surfaces --disable-full-testing"
ABSHADOW=no
NOLTO=yes
NOLTO=yes
ABHOST=noarch
