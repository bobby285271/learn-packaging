PKGNAME=darling
PKGSEC=utils
PKGDEP="gnustep-corebase libbsd systemd openssl"
BUILDDEP="32subsystem"
PKGDES="Wine-like emulation layer that enables you to run Darwin/OSX apps on Linux"

