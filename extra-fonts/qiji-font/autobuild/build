wget https://github.com/LingDong-/qiji-font/releases/download/v$PKGVER/qiji.ttf
wget https://github.com/LingDong-/qiji-font/releases/download/v$PKGVER/qiji-fallback.ttf
wget https://github.com/LingDong-/qiji-font/releases/download/v$PKGVER/qiji-combo.ttf

install -dm755 "$PKGDIR"/usr/share/fonts/TTF
install -vm644 *.ttf "$PKGDIR"/usr/share/fonts/TTF/
