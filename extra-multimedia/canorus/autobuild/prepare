export CFLAGS="${CFLAGS} -Wno-error=misleading-indentation -Wno-error=dangling-else -Wno-error=maybe-uninitialized -Wno-error=format-overflow= -Wno-error=deprecated-declarations"
export CXXFLAGS="${CXXFLAGS} -Wno-error=misleading-indentation -Wno-error=dangling-else -Wno-error=maybe-uninitialized -Wno-error=format-overflow= -Wno-error=deprecated-declarations"
