PKGNAME=libpng
PKGDES="Libraries used by other programs for reading and writing PNG files"
PKGDEP="zlib"
PKGSEC=libs

AUTOTOOLS_AFTER=""
AUTOTOOLS_AFTER__ARM=" \
                 --enable-arm-neon=api"
AUTOTOOLS_AFTER__ARM64="${AUTOTOOLS_AFTER__ARM}"
AUTOTOOLS_AFTER__ARMHF="${AUTOTOOLS_AFTER__ARM}"

ABSHADOW=0
AB_FLAGS_SPECS=0
RECONF=0
