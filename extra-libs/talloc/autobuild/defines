PKGNAME=talloc
PKGSEC=admin
PKGDEP="glibc python-2"
PKGDES="Hierarchical pool based memory allocator with destructors"

AUTOTOOLS_STRICT=0
AUTOTOOLS_AFTER="--prefix=/usr \
                 --sysconfdir=/etc/samba \
                 --localstatedir=/var \
                 --bundled-libraries=NONE \
                 --builtin-libraries=replace \
                 --enable-talloc-compat1"
ABSHADOW=0
NOPARALLEL=1
