install -Dm644 $PKGNAME-$PKGVER.jar "$PKGDIR"/usr/share/java/$PKGNAME/$PKGNAME.jar
ln -sv $PKGNAME.jar "$PKGDIR"/usr/share/java/$PKGNAME/commons-math.jar
ln -sv $PKGNAME "$PKGDIR"/usr/share/java/commons-math

